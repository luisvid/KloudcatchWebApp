#signin_form.modal.fade
  %a.close{:data => {:dismiss => "modal"}} x
  .tabbable
    %ul.nav.nav-tabs.white
      %li.active= link_to I18n.t(:signin), "#signin_tab", :data => {:toggle => "tab"}
      %li= link_to I18n.t(:signup), "#signup_tab", :data => {:toggle => "tab"}
    .tab-content.white
      #signin_tab.tab-pane.active= render :template => "sessions/new_modal"
      #signup_tab.tab-pane= render :template => "users/new_modal"
